%Luis Monroy
%Logical Arrays and Array Indexing
%cpe11.m

clear,clc,close all
%a
price_A = [19, 18, 22, 21, 25, 19, 17, 21, 27, 29];
price_B = [22, 17, 20, 19, 24, 18, 16, 25, 28, 27];
price_C = [17, 13, 22, 23, 19, 17, 20, 21, 24, 28];

days1=(price_A > price_B)&(price_A > price_C);
days1

%b
days2=(price_A > price_B)|(price_A > price_C);
days2

%c
daysAB=(price_A > price_B);
daysAC=(price_A > price_C);
days3=daysAB~=daysAC;
days3
